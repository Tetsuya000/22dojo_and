<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/node_graph"
    app:startDestination="@id/node">

    <fragment
        android:id="@+id/node"
        android:name="jp.gr.java.conf.tmproject.dojoandroid2022.presentation.ui.rodemap.node.RoadmapNodeFragment"
        android:label=""
        tools:layout="@layout/roadmap_node_fragment" >

        <argument
            android:name="nodes"
            app:nullable="true"
            app:argType="jp.gr.java.conf.tmproject.dojoandroid2022.domain.model.Node[]" />

        <action
            android:id="@+id/navigate_node_to_childNodes"
            app:destination="@id/node">
        </action>
    </fragment>
</navigation>